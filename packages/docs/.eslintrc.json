{
    "extends": "@sergio-ignite-call-ui/eslint-config",
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "printWidth": 100,
                "tabWidth": 4,
                "singleQuote": false,
                "trailingComma": "es5",
                "arrowParens": "always",
                "semi": true,
                "singleAttributePerLine": true
            }
        ]
    }
}
